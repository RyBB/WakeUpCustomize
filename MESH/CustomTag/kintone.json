{
	"formatVersion": "1.0",
	"tagData": {
		"name": "kintone (Tips)",
		"icon": "data:image/png;base64,",
		"description": "MESHからkintoneにデータを送信するサンプルカスタムタグです。",
		"functions": [
			{
				"id": "function_0",
				"name": "振ったらレコード登録",
				"connector": {
					"inputs": [
						{
							"label": ""
						}
					],
					"outputs": [
						{
							"label": ""
						}
					]
				},
				"properties": [
					{
						"name": "ドメイン名",
						"referenceName": "domain",
						"type": "string",
						"defaultValue": "{YOUR_CYBOZU_DOMAIN}.cybozu.com"
					},
					{
						"name": "アプリケーションID",
						"referenceName": "appId",
						"type": "string",
						"defaultValue": "{YOUR_APP_ID}"
					},
					{
						"name": "APIトークン",
						"referenceName": "apiToken",
						"type": "string",
						"defaultValue": "{YOUR_API_TOKEN}"
					},
					{
						"name": "kintoneログインID",
						"referenceName": "userName",
						"type": "string",
						"defaultValue": "{YOUR_CYBOZU_LOGIN_ID}"
					}
				],
				"extension": {
					"initialize": "",
					"receive": "",
					"execute": "const common = {\n  url: 'https://' + properties.domain + '/k/v1/record.json',\n  appId: properties.appId,\n  apiToken: properties.apiToken,\n  userName: properties.userName\n};\n\n// レコードに設定する値\nconst params = {\n\tapp: common.appId,\n\trecord: {\n\t\tUser: {\n\t\t\tvalue: [{\n\t\t\t\tcode: common.userName\n\t\t\t}]\n\t\t}\n\t}\n};\n\najax({\n  url : common.url,\n  data : JSON.stringify(params),\n  type : 'POST',\n  contentType : 'application/json',\n  dataType : 'json',\n  timeout : 5000,\n\theaders: {\n\t\t'X-Cybozu-API-Token': common.apiToken\n\t},\n\tsuccess: function() {\n    log('success');\n  },\n  error: function(request, errorMessage) {\n    log('error');\n\t\tlog(request.responseText);\n\t\tlog(errorMessage);\n  }\n});\nreturn {\n  resultType : 'continue'\n};",
					"result": ""
				}
			}
		]
	}
}